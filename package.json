    "@types/node": "^24.0.7",
    "@types/node-fetch": "^2.6.12",
    "@types/nodemailer": "^6.4.17",
    "dotenv-cli": "^7.4.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"

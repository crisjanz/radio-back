  "main": "index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "dev:local": "dotenv -e .env.development -- tsx --watch src/index.ts",
    "build": "npx prisma generate && npx prisma db push && tsc || true",
    "build:local": "dotenv -e .env.development -- npx prisma generate --schema=prisma/schema.dev.prisma && dotenv -e .env.development -- npx prisma db push --schema=prisma/schema.dev.prisma && tsc || true",
    "start": "node dist/index.js",
    "postinstall": "npx prisma generate",
    "db:deploy": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:studio:local": "dotenv -e .env.development -- npx prisma studio --schema=prisma/schema.dev.prisma",
    "db:init:local": "dotenv -e .env.development -- npx prisma generate --schema=prisma/schema.dev.prisma && dotenv -e .env.development -- npx prisma db push --schema=prisma/schema.dev.prisma",
    "db:seed:local": "dotenv -e .env.development -- node export-sqlite-data.js"
  },
  "keywords": [],
